    rdfs:range comic:Publisher .

comic:Publisher a owl:Class ;
    rdfs:label "Publisher"@en ;
    rdfs:comment "A class defining the publisher." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:string ;
            owl:onDataRange xsd:string ;
            owl:onProperty comic:publisherName ] .

comic:Comic a owl:Class ;
    rdfs:label "Comic"@en ;
    rdfs:comment "A class defining the comics." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:string ;
            owl:onProperty comic:hasTitle ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:nonNegativeInteger ;
            owl:onProperty comic:issueNumber ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:nonNegativeInteger ;
            owl:onProperty comic:hasComicID ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:string ;
            owl:onDataRange comic:Publisher ;
            owl:onProperty comic:hasPublisher ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:string ;
            owl:onDataRange xsd:string ;
            owl:onProperty comic:hasDescription ],
        dbo:Comic .

comic:Character a owl:Class ;
    rdfs:label "Character"@en ;
    rdfs:comment "A class defining the character." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:string ;
            owl:onDataRange xsd:string ;
            owl:onProperty comic:hasHairColor ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:nonNegativeInteger ;
            owl:onProperty comic:hasHeight ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:string ;
            owl:onDataRange xsd:string ;
            owl:onProperty comic:hasFormerlyDeceased ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:string ;
            owl:onDataRange xsd:string ;
            owl:onProperty comic:hasAlignment ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:string ;
            owl:onDataRange xsd:string ;
            owl:onProperty comic:hasEyeColor ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:string ;
            owl:onDataRange xsd:string ;
            owl:onProperty comic:hasGender ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:nonNegativeInteger ;
            owl:onProperty comic:hasCharacterID ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:string ;
            owl:onDataRange xsd:string ;
            owl:onProperty comic:hasLivingStatus ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:nonNegativeInteger ;
            owl:onProperty comic:hasWeight ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:string ;
            owl:onDataRange comic:Species ;
            owl:onProperty comic:hasSpecies ],
        dbo:ComicsCharacter .

comic:Species a owl:Class ;
    rdfs:label "Species"@en ;
    rdfs:comment "A class defining the species of the character." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:string ;
            owl:onDataRange xsd:string ;
            owl:onProperty comic:SpeciesName ] .

