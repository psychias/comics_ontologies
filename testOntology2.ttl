@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

comic:Marvel_Comics a comic:Publisher ;
    rdfs:label "Marvel Comics"@en ;
    comic:publisherName "Marvel Comics"@en ;
    rdfs:comment "A rescource describing Marvel comics." ;
    owl:sameAs dbr:Marvel_Comics .

comic:allDifferent a owl:AllDifferent ;
    owl:distinctMembers comic:Alien,
        comic:Animal,
        comic:Arthrosian,
        comic:Asgardian,
        comic:Atlantean,
        comic:Cyborg,
        comic:Deity,
        comic:Demon,
        comic:Duckworldian,
        comic:Eternal,
        comic:Faltine_hybrid,
        comic:Flora_Colossus,
        comic:Human,
        comic:Inhuman,
        comic:Kakarantharaian,
        comic:Korbinite,
        comic:Lumphomoid,
        comic:Mutant,
        comic:Neyaphem,
        comic:Robot,
        comic:Sakaaran_Shadow_People,
        comic:Skrull,
        comic:Strontian,
        comic:Symbiote,
        comic:Vampire,
        comic:Xandarian,
        comic:Zen-Whoberian .

comic:hasCharacters a owl:ObjectProperty ;
    owl:inverseOf comic:appearsIn .

comic:hasIssueNumber a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "issue number"@en ;
    rdfs:comment "The issue number of the comic." ;
    rdfs:domain comic:Comic ;
    rdfs:range xsd:nonNegativeInteger .

comic:speciesName a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "species name"@en ;
    rdfs:comment "The name of the species." ;
    rdfs:domain comic:Species ;
    rdfs:range comic:Species .

